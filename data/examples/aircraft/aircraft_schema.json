{
  "type": "object",
  "title": "Aircraft",
  "version": "1.0.0",
  "properties": {
    "metadata": {
      "description": "Metadata when the record is processed",
      "title": "Metadata",
      "type": "object"
    },
    "REGISTRATION": {
      "description": "Registration of the Aircraft",
      "examples": [
        "BHNR",
        "LD33F"
      ],
      "maxLength": 10,
      "title": "Registration",
      "type": "string"
    },
    "VALID_SINCE": {
      "description": "Start of validity",
      "format": "date-time",
      "title": "Valid Since",
      "type": "string"
    },
    "VALID_UNTIL": {
      "description": "End of validity",
      "format": "date-time",
      "title": "Valid Until",
      "type": "string"
    },
    "AC_OPERATOR": {
      "description": "Aircraft Operator",
      "examples": [
        "LD",
        "CX"
      ],
      "maxLength": 3,
      "title": "Ac Operator",
      "type": "string"
    },
    "AC_OWNER": {
      "description": "Owner of the aircraft",
      "examples": [
        "LD",
        "CX"
      ],
      "maxLength": 3,
      "title": "Ac Owner",
      "type": "string"
    },
    "AC_SUBTYPE": {
      "description": "Subtype of the aircraft",
      "maxLength": 3,
      "title": "Ac Subtype",
      "type": "string"
    },
