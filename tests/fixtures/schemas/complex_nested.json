{
  "type": "object",
  "description": "Complex nested structure with multiple levels",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "profile": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "address": {
              "type": "object",
              "properties": {
                "street": {
                  "type": "string"
                },
                "city": {
                  "type": "string"
                }
              }
            }
          },
          "required": ["name"]
        },
        "orders": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "orderId": {
                "type": "string"
              },
              "items": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "productId": {
                      "type": "integer"
                    },
                    "quantity": {
                      "type": "integer"
                    }
                  }
                }
              }
            },
            "required": ["orderId"]
          }
        }
      },
      "required": ["id", "profile"]
    }
  },
  "required": ["user"]
}

